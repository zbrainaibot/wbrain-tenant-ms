{
  "name": "wbrain-tenant-microservice",
  "version": "1.0.0",
  "description": "This is wbrain-tenant-microservice in node.js.",
  "main": "server.js",
  "scripts": {
    "start": "babel-node ./server.js",
    "test": "cross-env NODE_ENV=development mocha -r @babel/register -r regenerator-runtime/runtime --reporter json --reporter-options output=report.json --exit --timeout 10000",
    "nodemon": "nodemon --exec babel-node ./server.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "prepare": "husky install",
    "coverage": "nyc -r cobertura -r text-summary -r html npm run test",
    "coverage-lcov": "nyc -r cobertura -r text-summary -r lcov npm run test && rm -rf ./coverage/lcov-report"
  },
  "author": "Leewayhertz",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.626.0",
    "@azure/storage-blob": "^12.16.0",
    "amqplib": "^0.10.0",
    "axios": "^1.7.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron-master": "^1.0.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.27.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemon": "^1.19.2",
    "slack-notifier": "^0.0.3",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^9.0.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/register": "^7.24.6",
    "@types/luxon": "^3.2.0",
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^10.2.11",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prettier": "2.8.7",
    "regenerator-runtime": "^0.13.11"
  },
  "lint-staged": {
    "*.js": [
      "npm run test"
    ]
  }
}
